#ifndef BWC_CONFIG_H_
#define BWC_CONFIG_H_

/* This file is just a placeholder so as to force rebuilding of bwc when the
 * underlying MPI library changes. If we have an MPI build, then HAVE_MPI is
 * substituted with the directory where the mpiexec command has been found. */

#cmakedefine    HAVE_MPI "@HAVE_MPI@"

#endif	/* BWC_CONFIG_H_ */
