void mpfq_2_128_print(mpfq_2_128_field_ptr K MAYBE_UNUSED, FILE * f, mpfq_2_128_src_elt s)
{
	unsigned int j;
	int sth = 0;
	for(j = 0 ; j < 128 ; j++) {
		if (s[j/32] >> (j % 32) & 1UL) {
			if (sth) fprintf(f, " + ");
			sth = 1;
			if (j == 0) {
				fprintf(f, "1");
			} else if (j == 1) {
				fprintf(f, "z");
			} else {
				fprintf(f, "z^%d",j);
			}
		}
	}
	if (!sth) {
		fprintf(f, "0");
	}
	fprintf(f, "\n");
}

